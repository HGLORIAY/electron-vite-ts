{
  "compilerOptions": {
    "target": "es6",
    "useDefineForClassFields": true,
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "isolatedModules": false,
    "esModuleInterop": true,
    "lib": ["es6", "dom"],
    "skipLibCheck": true,
    "outDir": "dist/electron"
  },
  "include": ["electron/**/*"],
  "references": [{
    "path": "./tsconfig.node.json"
  }]
}